{% extends 'base.twig' %}

{% block title %}دفتر ترجمه رسمی شماره {{center.code}} {{center.city}}{% endblock %}
{% block scripts %}
<script type="text/javascript">
	$(document).ready(function(){  
          
	});
        
</script>
{% endblock %}


{% block content %}

<div class="container mainContainer">
        <h4>دفتر ترجمه رسمی شماره {{center.code}} {{center.city}}</h4>
	<div class="row">
            <div class="col-4">مترجم مسئول</div>
            <div class="col-8">{{center.manager}}</div>
	</div>
	<div class="row">
            <div class="col-4">زبان تخصصی</div>
